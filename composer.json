{
  "name": "pavelvais/snapshot-doctrine",
  "description": "Library for creating simple database snapshots and restoring them.",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Pavelvais\\SnapshotDoctrine\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Pavelvais\\UpsertDoctrine\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=8.0",
    "ext-mysqli": "*",
    "doctrine/orm": "^2.0",
    "doctrine/dbal": "^3.0"
  },
  "require-dev": {
    "doctrine/coding-standard": "^8.0 || ^9.0",
    "phpunit/phpunit": "^8.0 || ^9.0"
  },
  "authors": [
    {
      "name": "Pavel Vais"
    }
  ],
  "minimum-stability": "dev",
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
